<div id='signup_form' class="col-sm-6 col-sm-offset-3">
    <h1><span class="fa fa-sign-in"></span> ~Signup~</h1>
    <!-- show any messages that come back with authentication -->
    <% if (messagesign.length > 0) { %>
        <div class="alert alert-danger"><%= messagesingn %></div>
    <% } %>
    <!-- LOGIN FORM -->
        <div class="form-group">
            <label>Email</label>
            <input id='signup_email' type="text" class="form-control" name="email">
        </div>
        <div class="form-group">
            <label>Password</label>
            <input id='signup_password' type="password" class="form-control" name="password">
        </div>
        <button id='singup' class="btn btn-warning btn-lg">Signup</button>
    <hr>
    <p>Already have an account? <a href="javascript:fillAjax('/login_one','main')">Login</a></p>
</div>